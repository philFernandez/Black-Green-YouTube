{
	"name": "Black and Green YouTube",
	"version": "0.0.0.1",
	"manifest_version": 2,
	"description": "A black and green theme for YouTube",
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			// "js": ["script.js"],
			"css": ["style.css"],
			"matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
			"include_globs": ["*"],
			"run_at": "document_start",
			"all_frames": true			
		}
	],
	"icons": {				
		"48":"icon48.png"
	}
}